<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cs2340"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" >
    </uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.activities.LoginActivity" >
        </activity>
        <activity android:name="com.example.cs2340.LoginSuccessfulView" >
        </activity>
        <activity android:name="com.example.activities.RegisterActivity" >
        </activity>
        <activity
            android:name="com.example.activities.AccountMain" >
        </activity>
        <activity android:name="com.example.activities.ViewAccountsActivity"></activity>
        <activity android:name="com.example.activities.AccountCreationActivity"></activity>
        <activity android:name="com.example.activities.MakeTransactionActivity"></activity>
    </application>

</manifest>
